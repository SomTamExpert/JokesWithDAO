<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.text.Text?>
<Pane prefHeight="650.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="bbw.km.PrimaryController">
    <children>
        <Label alignment="CENTER" layoutX="128.0" layoutY="14.0" prefHeight="27.0" text="DAO Pattern with Jokes"
               textAlignment="CENTER">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Label layoutX="18.0" layoutY="60.0" text="Jokes from SQL Database:">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Label>
        <Label layoutX="21.0" layoutY="111.0" text="Id:"/>
        <TextField fx:id="txtId" layoutX="81.0" layoutY="107.0" prefHeight="25.0" prefWidth="250.0"/>
        <Label layoutX="21.0" layoutY="151.0" text="content:"/>
        <TextField fx:id="txtJoke" layoutX="81.0" layoutY="147.0" prefHeight="25.0" prefWidth="250.0"/>
        <Label layoutX="18.0" layoutY="191.0" text="Date:"/>
        <TextField fx:id="txtDate" layoutX="81.0" layoutY="187.0" prefHeight="25.0" prefWidth="250.0"/>
        <Label layoutX="18.0" layoutY="231.0" text="Rating:"/>
        <TextField fx:id="txtRating" layoutX="81.0" layoutY="227.0" prefHeight="25.0" prefWidth="250.0"/>
        <Button layoutX="349.0" layoutY="107.0" onAction="#listAllJokes" prefHeight="25.0" prefWidth="65.0"
                text="List"/>
        <Button layoutX="349.0" layoutY="147.0" onAction="#addJoke" prefHeight="25.0" prefWidth="65.0" text="Add"/>
        <Button layoutX="349.0" layoutY="187.0" onAction="#deleteJoke" prefHeight="25.0" prefWidth="65.0"
                text="Delete"/>
        <Button layoutX="349.0" layoutY="227.0" onAction="#updateJoke" prefHeight="25.0" prefWidth="65.0"
                text="Update"/>
        <ScrollPane layoutX="16.0" layoutY="300.0" prefHeight="261.0" prefWidth="968.0">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="300.0" prefWidth="968.0">
                    <children>
                        <Label alignment="TOP_LEFT" prefHeight="300.0" prefWidth="968.0"
                               style="-fx-background-color: #efc011#efc011;"/>
                        <Text fx:id="lblOut" layoutY="15"/>
                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
        <Button layoutX="532.0" layoutY="600.0" onAction="#switchToSecondary" text="To Jokes From Access Database"/>
        <Button layoutX="532.0" layoutY="600.0" onAction="#switchToThirdly" text="To Jokes From Mongo Database"/>
    </children>
</Pane>
